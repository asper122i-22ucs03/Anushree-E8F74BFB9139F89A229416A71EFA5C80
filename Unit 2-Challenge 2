#define base class player
class player:
  def play(self):
    print("the player is playing cricket.")
#define the derived class batsman
class batsman(player):
  def play(self):
    print("the batsman is batting.")
#define the derived class bolwer
class bolwer(player):
  def play(self):
    print("the bolwer is bolwing.")
#call the objects of batsman and bolwer class
batsman=batsman()
bolwer=bolwer()
#call the play()method for each object
batsman.play()
bolwer.play()
    
    
